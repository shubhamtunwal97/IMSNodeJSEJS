<% include partials/header.ejs %>
<br><br>
<div class="container">
  <table class="table table-striped table-bordered table-hover">
    <tr>
      <td colspan="2" align="center"><strong><%= code %></strong></td>
      <td colspan="3" align="center"><strong><%= name %></strong></td>
    </tr>
    <thead class="thead-dark">
      <tr>
        <th>#</th>
        <th>Part Number</th>
        <th>Description</th>
        <th>Stock</th>
        <th>Quantity</th>
      </tr>
    </thead>
    <tbody>
    <% for(var i=0;i<raw_materials.length;i++){ %>
      <tr>
        <td><%= i+1 %></td>
        <td><%= raw_materials[i].raw_material_code %></td>
        <td><%= raw_materials[i].name %></td>
        <td><%= raw_materials[i].stock %></td>
        <td><%= raw_materials[i].quantity %></td>
      </tr>
    <% } %>
    </tbody>
  </table>
</div>
<script type="text/javascript">
	new TableExport(document.getElementsByTagName("table"),{bootstrap:false,headers:true,formats:['csv','xls','xlsx'],ignoreCols:[4]});
</script>
<% include partials/footer.ejs %>
