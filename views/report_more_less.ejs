<% include partials/header.ejs %>
<br>
<h1 align="center">List of <%= type %> Raw Material</h1>
<div class="container">
	<table class="table table-striped table-bordered table-hover" id="table">
		<thead class="thead-dark">
			<tr>
				<th>#</th>
				<th>Description</th>
				<th>Part Number</th>
				<th>DTPL Part Number</th>
				<th>Category</th>
				<th>Stock</th>
				<th>Monthly Requirement</th>
				<th>Unit</th>
				<th>Supplier</th>
			</tr>
		</thead>
		<tbody>
			<% for(var i=0;i<raw_materials.length;i++){ %>
				<tr>
					<td><%= i+1 %></td>
					<td><%= raw_materials[i].name %></td>
					<td><%= raw_materials[i].code %></td>
					<td><%= raw_materials[i].DTPL_code %></td>
					<td><%= raw_materials[i].category %></td>
					<td><%= raw_materials[i].stock %></td>
					<td><%= raw_materials[i].monthly_requirement %></td>
					<td><%= raw_materials[i].unit %></td>
					<td><%= raw_materials[i].supplier_code %></td>
				</tr>
			<% } %>
		</tbody>
	</table>
</div>
<script type="text/javascript">
	new TableExport(document.getElementsByTagName("table"),{bootstrap:false,headers:true,formats:['csv','xls','xlsx'],ignoreCols:[0]});
</script>
<% include partials/footer.ejs %>
