<% include partials/header.ejs %>
<br>
<h1 align="center">Report By PO</h1>
<br>
<div class="container">
	<table class="table table-striped table-bordered table-hover">
		<thead class="thead-dark">
			<tr>
				<th>PO Number</th>
				<th>Invoice Number</th>
				<th>DTPL Part Number</th>
				<th>Description</th>
				<th>Quantity</th>
				<th>Date</th>
			</tr>
		</thead>
		<tbody>
			<% for(var i=0;i<inputs.length;i++){ %>
				<tr>
					<td><%= inputs[i].PO_code %></td>
					<td><%= inputs[i].invoice_no %></td>
					<td><%= inputs[i].DTPL_code %></td>
					<td><%= inputs[i].raw_desc %></td>
					<td><%= inputs[i].quantity %></td>
					<td><%= inputs[i].date.toISOString().substring(0,10) %></td>
				</tr>
			<% } %>
		</tbody>
	</table>
</div>
<script type="text/javascript">
	new TableExport(document.getElementsByTagName("table"),{bootstrap:false,headers:true,formats:['csv','xls','xlsx'],ignoreCols:[]});
</script>
<% include partials/footer.ejs %>