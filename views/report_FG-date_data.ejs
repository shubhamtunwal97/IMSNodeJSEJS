<% include partials/header.ejs %>
<br><br>
<h1 align="center"><%= type %> Report</h1>
<br><br>
<div class="container">
  <table class="table table-striped table-bordered table-hover">
    <thead class="thead-dark">
      <th>#</th>
      <th>Part Number</th>
      <th>Quantity</th>
      <th>Date</th>
    </thead>
    <tbody>
      <tr>
        <td></td>
        <td></td>
        <td></td>
        <td>From: <%= from %></td>
      </tr>
      <% for(var i=0;i<finished_goods.length;i++){ %>
        <tr>
          <td><%= i+1 %></td>
          <td><%= finished_goods[i].FG_code %></td>
          <td><%= finished_goods[i].quantity %></td>
          <td><%= finished_goods[i].date %></td>
        </tr>
      <% } %>
      <tr>
        <td></td>
        <td></td>
        <td></td>
        <td>To: <%= to %></td>
      </tr>
    </tbody>
  </table>
</div>
<script type="text/javascript">
	new TableExport(document.getElementsByTagName("table"),{bootstrap:false,headers:true,formats:['csv','xls','xlsx'],ignoreCols:[]});
</script>
<% include partials/footer.ejs %>
